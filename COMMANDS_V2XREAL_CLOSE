# ========== Scene 16｜train4/2023-04-05-16-16-26_13_1（12 cams） ==========
python datasets/tools/v2xreal_to_processed_multicam.py --source_root data/v2x-real/raw --split train4 --scenario 2023-04-05-16-16-26_13_1 --vehicle_ids 1 2 --infra_ids -1 -2 --target_root data/v2x-real/processed_multicam --scene_idx 16 --instances_source union --expected_cams 12 --merge_vehicle_lidar --merge_lidar_max_points 200000
python datasets/tools/v2xreal_generate_dynamic_masks.py --processed_scene data/v2x-real/processed_multicam/016
conda activate segformer
python datasets/tools/extract_masks.py --data_root data/v2x-real/processed_multicam --scene_ids 16 --segformer_path ./SegFormer --config ./SegFormer/local_configs/segformer/B5/segformer.b5.1024x1024.city.160k.py --checkpoint ./SegFormer/pretrained/segformer.b5.1024x1024.city.160k.pth --rgb_dirname images --mask_dirname fine_dynamic_masks --process_dynamic_mask
conda activate 4D-humans
python datasets/tools/humanpose_process.py --dataset v2xreal --data_root data/v2x-real/processed_multicam --scene_ids 16 --save_temp
conda activate drivestudio
python tools/train.py --config_file configs/omnire.yaml --output_root work_dirs --project drivestudio --run_name v2x-real-train4-2023-04-05-16-16-26_13_1 dataset=v2xreal/12cams data.data_root=data/v2x-real/processed_multicam data.scene_idx=16 data.pixel_source.load_smpl=True

