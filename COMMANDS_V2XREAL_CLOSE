#!/usr/bin/env bash
# V2X-Real 20 场景流水线（逐场景顺序执行：转换 → 掩码 → 人体 → 训练）
# 已按两车平均距离由近到远排序，且已移除所有 8-cams 场景；scene_idx 从 0 ~ 19。

export PYTHONPATH=$(pwd)

# ========== Scene 00｜validate/2023-04-04-15-43-17_15_0（12 cams） ==========
python datasets/tools/v2xreal_to_processed_multicam.py --source_root data/v2x-real/raw --split validate --scenario 2023-04-04-15-43-17_15_0 --vehicle_ids 1 2 --infra_ids -1 -2 --target_root data/v2x-real/processed_multicam --scene_idx 0 --instances_source union --expected_cams 12 --merge_vehicle_lidar --merge_lidar_max_points 200000
python datasets/tools/v2xreal_generate_dynamic_masks.py --processed_scene data/v2x-real/processed_multicam/000
conda activate segformer
python datasets/tools/extract_masks.py --data_root data/v2x-real/processed_multicam --scene_ids 0 --segformer_path ./SegFormer --config ./SegFormer/local_configs/segformer/B5/segformer.b5.1024x1024.city.160k.py --checkpoint ./SegFormer/pretrained/segformer.b5.1024x1024.city.160k.pth --rgb_dirname images --mask_dirname fine_dynamic_masks --process_dynamic_mask
conda activate 4D-humans
python datasets/tools/humanpose_process.py --dataset v2xreal --data_root data/v2x-real/processed_multicam --scene_ids 0 --save_temp
conda activate drivestudio
python tools/train.py --config_file configs/omnire.yaml --output_root work_dirs --project drivestudio --run_name v2x-real-validate-2023-04-04-15-43-17_15_0 dataset=v2xreal/12cams data.data_root=data/v2x-real/processed_multicam data.scene_idx=0 data.pixel_source.load_smpl=True

# ========== Scene 01｜test/2023-04-04-15-47-17_19_0（12 cams） ==========
python datasets/tools/v2xreal_to_processed_multicam.py --source_root data/v2x-real/raw --split test --scenario 2023-04-04-15-47-17_19_0 --vehicle_ids 1 2 --infra_ids -1 -2 --target_root data/v2x-real/processed_multicam --scene_idx 1 --instances_source union --expected_cams 12 --merge_vehicle_lidar --merge_lidar_max_points 200000
python datasets/tools/v2xreal_generate_dynamic_masks.py --processed_scene data/v2x-real/processed_multicam/001
conda activate segformer
python datasets/tools/extract_masks.py --data_root data/v2x-real/processed_multicam --scene_ids 1 --segformer_path ./SegFormer --config ./SegFormer/local_configs/segformer/B5/segformer.b5.1024x1024.city.160k.py --checkpoint ./SegFormer/pretrained/segformer.b5.1024x1024.city.160k.pth --rgb_dirname images --mask_dirname fine_dynamic_masks --process_dynamic_mask
conda activate 4D-humans
python datasets/tools/humanpose_process.py --dataset v2xreal --data_root data/v2x-real/processed_multicam --scene_ids 1 --save_temp
conda activate drivestudio
python tools/train.py --config_file configs/omnire.yaml --output_root work_dirs --project drivestudio --run_name v2x-real-test-2023-04-04-15-47-17_19_0 dataset=v2xreal/12cams data.data_root=data/v2x-real/processed_multicam data.scene_idx=1 data.pixel_source.load_smpl=True

# ========== Scene 02｜test/2023-04-04-14-27-53_44_0（12 cams） ==========
python datasets/tools/v2xreal_to_processed_multicam.py --source_root data/v2x-real/raw --split test --scenario 2023-04-04-14-27-53_44_0 --vehicle_ids 1 2 --infra_ids -1 -2 --target_root data/v2x-real/processed_multicam --scene_idx 2 --instances_source union --expected_cams 12 --merge_vehicle_lidar --merge_lidar_max_points 200000
python datasets/tools/v2xreal_generate_dynamic_masks.py --processed_scene data/v2x-real/processed_multicam/002
conda activate segformer
python datasets/tools/extract_masks.py --data_root data/v2x-real/processed_multicam --scene_ids 2 --segformer_path ./SegFormer --config ./SegFormer/local_configs/segformer/B5/segformer.b5.1024x1024.city.160k.py --checkpoint ./SegFormer/pretrained/segformer.b5.1024x1024.city.160k.pth --rgb_dirname images --mask_dirname fine_dynamic_masks --process_dynamic_mask
conda activate 4D-humans
python datasets/tools/humanpose_process.py --dataset v2xreal --data_root data/v2x-real/processed_multicam --scene_ids 2 --save_temp
conda activate drivestudio
python tools/train.py --config_file configs/omnire.yaml --output_root work_dirs --project drivestudio --run_name v2x-real-test-2023-04-04-14-27-53_44_0 dataset=v2xreal/12cams data.data_root=data/v2x-real/processed_multicam data.scene_idx=2 data.pixel_source.load_smpl=True

# ========== Scene 03｜train1/2023-03-17-16-11-12_2_1（12 cams） ==========
python datasets/tools/v2xreal_to_processed_multicam.py --source_root data/v2x-real/raw --split train1 --scenario 2023-03-17-16-11-12_2_1 --vehicle_ids 1 2 --infra_ids -1 -2 --target_root data/v2x-real/processed_multicam --scene_idx 3 --instances_source union --expected_cams 12 --merge_vehicle_lidar --merge_lidar_max_points 200000
python datasets/tools/v2xreal_generate_dynamic_masks.py --processed_scene data/v2x-real/processed_multicam/003
conda activate segformer
python datasets/tools/extract_masks.py --data_root data/v2x-real/processed_multicam --scene_ids 3 --segformer_path ./SegFormer --config ./SegFormer/local_configs/segformer/B5/segformer.b5.1024x1024.city.160k.py --checkpoint ./SegFormer/pretrained/segformer.b5.1024x1024.city.160k.pth --rgb_dirname images --mask_dirname fine_dynamic_masks --process_dynamic_mask
conda activate 4D-humans
python datasets/tools/humanpose_process.py --dataset v2xreal --data_root data/v2x-real/processed_multicam --scene_ids 3 --save_temp
conda activate drivestudio
python tools/train.py --config_file configs/omnire.yaml --output_root work_dirs --project drivestudio --run_name v2x-real-train1-2023-03-17-16-11-12_2_1 dataset=v2xreal/12cams data.data_root=data/v2x-real/processed_multicam data.scene_idx=3 data.pixel_source.load_smpl=True

